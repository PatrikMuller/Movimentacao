<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @*<link href="~/Content/Style/Site.css" rel="stylesheet" type="text/css" />*@
    @*<link href="~/Content/Style/bootstrap.min.css" rel="stylesheet" type="text/css" />*@
    @*<script src="~/Scripts/modernizr-2.6.2.js"></script>*@

    <link href="~/Content/SB/Vendor/css/bootstrap.min.css" rel="stylesheet" />

    <link href="~/Content/SB/Vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet" />

    <link href="~/Content/SB/Vendor/datatables/dataTables.bootstrap4.css" rel="stylesheet" />

    <link href="~/Content/SB/css/sb-admin.css" rel="stylesheet" />

</head>
<body class="fixed-nav sticky-footer">
    @*<div class="navbar navbar-inverse navbar-fixed-top">*@
    <div class="navbar navbar-expand-lg navbar-dark bg-dark fixed-top">

        @Html.ActionLink("Application name", "Index", "Home", null, new { @class = "navbar-brand" })

    </div>

    <div class="container body-content">
        @*<div class="content-wrapper">*@
            <div class="container-fluid">
                              
                <div class="row mb-3"></div>

                @RenderBody()

            </div>
            <hr />
            <footer>
                <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
            </footer>
        </div>

    @*@RenderSection("Scritps", false)*@

    @*<script src="~/Content/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Content/Scripts/bootstrap.min.js"></script>*@

    <script src="~/Content/SB/Vendor/jquery/jquery.min.js"></script>

    <script src="~/Content/SB/Vendor/js/bootstrap.bundle.min.js"></script>

    <script src="~/Content/SB/Vendor/jquery-easing/jquery.easing.min.js"></script>

    <script src="~/Content/SB/Vendor/datatables/jquery.dataTables.js"></script>

    <link href="~/Content/SB/Vendor/datatables/dataTables.bootstrap4.css" rel="stylesheet" />

    <script src="~/Content/SB/js/sb-admin.min.js"></script>

    <script src="~/Content/SB/js/sb-admin-datatables.min.js"></script>

</body>
</html>